# ProxyCommand ssh user@proxy nc %h %p 2> /dev/null
Host *
	KbdInteractiveAuthentication=no
	ForwardAgent yes
	AddressFamily=inet
	CheckHostIP=yes
	ServerAliveCountMax=6
	ServerAliveInterval=60
	TCPKeepAlive=yes
	#VerifyHostKeyDNS=yes

Host yggdrasil yggdrasil.assarbad.net
	HostName yggdrasil.assarbad.net
	Compression=yes
	CompressionLevel=9

Host sif gandalfr sif.yggdrasil gandalfr.yggdrasil
	Port 22022
	Compression=yes
	CompressionLevel=6

Host sif.yggdrasil gandalfr.yggdrasil
	ProxyCommand ssh %u@yggdrasil.assarbad.net nc %h %p 2> /dev/null

Host sif sif.yggdrasil
	HostKeyAlias sif
	HostName sif.assarbad.net

Host gandalfr gandalfr.yggdrasil
	HostKeyAlias gandalfr
	HostName gandalfr.assarbad.net

Host yggdrasil-old yggdrasil-old.assarbad.net
	HostName yggdrasil-old.assarbad.net
	Compression=yes
	CompressionLevel=9
